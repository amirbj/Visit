<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TableLayout
        android:background="@drawable/info_table_shape_1"
        android:padding="5dp"
        android:id="@+id/table_five"
        android:stretchColumns="*"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <TableRow
            android:layout_gravity="end">
            <TextView android:text="@string/non_deadline_debt"
                android:background="@drawable/dark_gray_shape"
                android:padding="5dp"
                android:layout_column="0"
                android:layout_weight="1"/>
            <TextView android:text="@string/deadline_debt"
                android:background="@drawable/dark_gray_shape"
                android:padding="5dp"
                android:layout_column="1"
                android:layout_weight="1"/>
            <TextView android:text="@string/debt_aggregation"
                android:background="@drawable/dark_gray_shape"
                android:padding="5dp"
                android:layout_column="2"
                android:layout_weight="1"/>
            <TextView android:text="@string/debt_status"
                android:background="@drawable/dark_gray_shape"
                android:padding="5dp"
                android:layout_column="3"
                android:layout_weight="1"/>
        </TableRow>
        <TableRow
            android:layout_gravity="end">
            <TextView
                android:background="@drawable/cell_shape"
                android:padding="5dp"
                android:id="@+id/energy_non_deadline_debt"
                android:textSize="15sp"
                android:layout_column="0"
                android:layout_weight="1"/>
            <TextView
                android:background="@drawable/cell_shape"
                android:padding="5dp"
                android:id="@+id/energy_deadline_debt"
                android:textSize="15sp"
                android:layout_column="1"
                android:layout_weight="1"/>
            <TextView
                android:background="@drawable/cell_shape"
                android:padding="5dp"
                android:id="@+id/energy_debt"
                android:textSize="15sp"
                android:layout_column="2"
                android:layout_weight="1"/>
            <TextView
                android:background="@drawable/dark_gray_shape"
                android:padding="5dp"
                android:text="@string/energy"
                android:textSize="15sp"
                android:layout_column="3"
                android:layout_weight="1"/>
        </TableRow>
        <TableRow
            android:layout_gravity="end">
            <TextView
                android:background="@drawable/cell_shape"
                android:padding="5dp"
                android:id="@+id/branch_non_deadline_debt"
                android:textSize="15sp"
                android:layout_column="0"
                android:layout_weight="1"/>
            <TextView
                android:background="@drawable/cell_shape"
                android:padding="5dp"
                android:id="@+id/branch_deadline_debt"
                android:textSize="15sp"
                android:layout_column="1"
                android:layout_weight="1"/>
            <TextView
                android:background="@drawable/cell_shape"
                android:padding="5dp"
                android:id="@+id/branch_debt"
                android:textSize="15sp"
                android:layout_column="2"
                android:layout_weight="1"/>
            <TextView
                android:background="@drawable/dark_gray_shape"
                android:padding="5dp"
                android:text="@string/branch"
                android:textSize="15sp"
                android:layout_column="3"
                android:layout_weight="1"/>
        </TableRow>
        <TableRow
            android:layout_gravity="end">
            <TextView
                android:background="@drawable/cell_shape"
                android:padding="5dp"
                android:id="@+id/other_non_deadline_debt"
                android:textSize="15sp"
                android:layout_column="0"
                android:layout_weight="1"/>
            <TextView
                android:background="@drawable/cell_shape"
                android:padding="5dp"
                android:id="@+id/other_deadline_debt"
                android:textSize="15sp"
                android:layout_column="1"
                android:layout_weight="1"/>
            <TextView
                android:background="@drawable/cell_shape"
                android:padding="5dp"
                android:id="@+id/other_debt"
                android:textSize="15sp"
                android:layout_column="2"
                android:layout_weight="1"/>
            <TextView
                android:background="@drawable/dark_gray_shape"
                android:padding="5dp"
                android:text="@string/other_services"
                android:textSize="15sp"
                android:layout_column="3"
                android:layout_weight="1"/>
        </TableRow>
        <TableRow
            android:layout_gravity="end">
            <TextView
                android:background="@drawable/dark_gray_shape"
                android:padding="5dp"
                android:id="@+id/aggregate_non_deadline_debt"
                android:textSize="15sp"
                android:layout_column="0"
                android:layout_weight="1"/>
            <TextView
                android:background="@drawable/dark_gray_shape"
                android:padding="5dp"
                android:id="@+id/aggregate_deadline_debt"
                android:textSize="15sp"
                android:layout_column="1"
                android:layout_weight="1"/>
            <TextView
                android:background="@drawable/dark_gray_shape"
                android:padding="5dp"
                android:id="@+id/aggregate_debt"
                android:textSize="15sp"
                android:layout_column="2"
                android:layout_weight="1"/>
            <TextView
                android:background="@drawable/dark_gray_shape"
                android:padding="5dp"
                android:text="@string/aggregate"
                android:textSize="15sp"
                android:layout_column="3"
                android:layout_weight="1"/>
        </TableRow>
    </TableLayout>

</FrameLayout>
